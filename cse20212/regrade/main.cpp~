#include <iostream>
#include "C4Board.h"

using namespace std;

int main() {
  C4Board C4;
  C4Board copy(C4);

  cout << "Number of columns in C4 object: " << C4.no_cols << endl;
  cout << "Number of columns in copy: " << copy.no_cols << endl << endl << endl;
  C4.play();
}
